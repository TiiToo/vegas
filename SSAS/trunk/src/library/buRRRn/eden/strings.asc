/* NameSpace: rez
   Configure the eden library messages.
   
   exemple:
   (code)
   trace( String.format( buRRRn.eden.rez.pairIsIgnored, "toto", 5, 10 ) );
   (end)
   
   attention:
   Don't use eden to load its own resources message
   this will cause an infinite loop.
*/
buRRRn.eden.rez = {};

/* Property: requirePairValue
   multiSerialize require pairs of values
*/
buRRRn.eden.rez.requirePairValue     = "multiSerialize require pairs of values";

/* Property: pairIsIgnored
   name "{0}" is not a string, pair[{1},{2}] is ignored
*/
buRRRn.eden.rez.pairIsIgnored        = "name \"{0}\" is not a string, pair[{1},{2}] is ignored";

/* Property: reservedKeyword
   "{0}" is a reserved keyword
*/
buRRRn.eden.rez.reservedKeyword      = "\"{0}\" is a reserved keyword";

/* Property: futurReservedKeyword
   "{0}" is a future reserved keyword
*/
buRRRn.eden.rez.futurReservedKeyword = "\"{0}\" is a future reserved keyword";

/* Property: notValidPath
   "{0} is not a valid path
*/
buRRRn.eden.rez.notValidPath         = "\"{0}\" is not a valid path";

/* Property: unterminatedComment
   unterminated comment
*/
buRRRn.eden.rez.unterminatedComment  = "unterminated comment";

/* Property: errorComment
   syntax error (comment)
*/
buRRRn.eden.rez.errorComment         = "syntax error (comment)";

/* Property: errorIdentifier
   bad identifier
*/
buRRRn.eden.rez.errorIdentifier      = "bad identifier";

/* Property: notValidConstructor
   "{0}" is not a valid constructor
*/
buRRRn.eden.rez.notValidConstructor  = "\"{0}\" is not a valid constructor";

/* Property: doesNotExist
   "{0}" does not exists
*/
buRRRn.eden.rez.doesNotExist         = "\"{0}\" does not exists";

/* Property: errorConstructor
   bad constructor
*/
buRRRn.eden.rez.errorConstructor     = "bad constructor";

/* Property: errorLineTerminator
   bad string (found line terminator in string)
*/
buRRRn.eden.rez.errorLineTerminator  = "bad string (found line terminator in string)";

/* Property: errorString
   bad string (unterminated string)
*/
buRRRn.eden.rez.errorString          = "bad string (unterminated string)";

/* Property: errorArray
   bad array (unterminated array)
*/
buRRRn.eden.rez.errorArray           = "bad array (unterminated array)";

/* Property: errorObject
   bad object (unterminated object)
*/
buRRRn.eden.rez.errorObject          = "bad object (unterminated object)";

/* Property: malformedHexadecimal
   bad number (malformed hexadecimal)
*/
buRRRn.eden.rez.malformedHexadecimal = "bad number (malformed hexadecimal)";

/* Property: errorNumber
   bad number (not finite)
*/
buRRRn.eden.rez.errorNumber          = "bad number (not finite)";

/* Property: extRefDoesNotExist
   external reference "{0}" does not exists
*/
buRRRn.eden.rez.extRefDoesNotExist   = "external reference \"{0}\" does not exists";

/* Property: errorKeyword
   syntax error
*/
buRRRn.eden.rez.errorKeyword         = "syntax error";

