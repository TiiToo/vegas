Tests.asgard.net.TestNetServerInfo = function( name ) 
{
    buRRRn.ASTUce.TestCase.call( this, name ) ;
}

Tests.asgard.net.TestNetServerInfo.prototype = new buRRRn.ASTUce.TestCase() ;
Tests.asgard.net.TestNetServerInfo.prototype.constructor = Tests.asgard.net.TestNetServerInfo ;

Tests.asgard.net.TestNetServerInfo.prototype.testConstructor = function () 
{
	
	var i ;
	
	i = new asgard.net.NetServerInfo() ;
	this.assertNotNull( i , "NSI_01_01 - constructor failed with a null value." )
	this.assertTrue( i instanceof asgard.net.NetServerInfo, "NSI_01_02 - constructor failed." ) ;
	
	var init = 
	{
		application : "vegas" ,
		code        : "code"  ,
		level       : "level" ,
		description : "the description"	
	}
	
	i = new asgard.net.NetServerInfo( init ) ;
	
	this.assertEquals( init.application , i.application , "NSI_01_03 - constructor failed with the 'application' attribut of the init argument of the constructor." )
	this.assertEquals( init.code        , i.code        , "NSI_01_04 - constructor failed with the 'code' attribut of the init argument of the constructor." )
	this.assertEquals( init.level       , i.level       , "NSI_01_05 - constructor failed with the 'level' attribut of the init argument of the constructor." )
	this.assertEquals( init.description , i.description , "NSI_01_06 - constructor failed with the 'description' attribut of the init argument of the constructor." )
	
}

Tests.asgard.net.TestNetServerInfo.prototype.testInherit = function () 
{
	var e = new asgard.net.NetServerInfo() ;
	this.assertTrue(e instanceof vegas.core.CoreObject, "NSI_02 - inherit failed." ) ;
}


