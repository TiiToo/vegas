// ---o Constructor

Tests.vegas.logging.TestLogEvent = function( name ) {
    buRRRn.ASTUce.TestCase.call( this, name ) ;
}

// ----o Inherit

Tests.vegas.logging.TestLogEvent.prototype = new buRRRn.ASTUce.TestCase() ;
Tests.vegas.logging.TestLogEvent.prototype.constructor = Tests.vegas.logging.TestLogEvent ;

// ----o Public Methods

Tests.vegas.logging.TestLogEvent.prototype.testConstructor = function () {
	
	var o = new vegas.logging.LogEvent() ;
	this.assertTrue( o , "LOG_EV_01 : constructor > " + o ) ;
	
}

Tests.vegas.logging.TestLogEvent.prototype.testInherit = function () {
	
	var o = new vegas.logging.LogEvent() ;
	var b = o instanceof vegas.events.DynamicEvent ;
	this.assertTrue( b , "LOG_EV_02 : inherit DynamicEvent.") ;
	
}

Tests.vegas.logging.TestLogEvent.prototype.testHashCode = function () {
	
	var o = new vegas.logging.LogEvent() ;
	var result = o.hashCode() ;
	this.assertTrue( !isNaN(result) , "LOG_EV_03 : hashCode() : " + result ) ;
	
}

Tests.vegas.logging.TestLogEvent.prototype.testToSource = function () {

	var o = new vegas.logging.LogEvent() ;
	var result = o.toSource() ;
	this.assertTrue( result , "LOG_EV_04 : toSource() : " + result) ;
	
}

Tests.vegas.logging.TestLogEvent.prototype.testClone = function () {
	
	var o = new vegas.logging.LogEvent() ;
	var clone = o.clone() ;
	this.assertTrue( clone instanceof vegas.logging.LogEvent , "LOG_EV_05 : clone is a LogEvent.") ;
	
}

Tests.vegas.logging.TestLogEvent.prototype.TestGetLevelString = function () {
	
	var s = vegas.logging.LogEvent.getLevelString ( vegas.logging.LogEventLevel.ALL ) ;
	this.assertEquals( s, "ALL" , "LOG_EV_06_01 : LogEventLevel.ALL : " + s ) ;

	var s = vegas.logging.LogEvent.getLevelString ( vegas.logging.LogEventLevel.DEBUG ) ;
	this.assertEquals( s, "DEBUG" , "LOG_EV_06_02 : LogEventLevel.DEBUG : " + s ) ;

	var s = vegas.logging.LogEvent.getLevelString ( vegas.logging.LogEventLevel.ERROR ) ;
	this.assertEquals( s, "ERROR" , "LOG_EV_06_03 : LogEventLevel.ERROR : " + s ) ;
	
	var s = vegas.logging.LogEvent.getLevelString ( vegas.logging.LogEventLevel.FATAL ) ;
	this.assertEquals( s, "FATAL" , "LOG_EV_06_04 : LogEventLevel.FATAL : " + s ) ;
	
	var s = vegas.logging.LogEvent.getLevelString ( vegas.logging.LogEventLevel.INFO ) ;
	this.assertEquals( s, "INFO" , "LOG_EV_06_05 : LogEventLevel.INFO : " + s ) ;
	
	var s = vegas.logging.LogEvent.getLevelString ( vegas.logging.LogEventLevel.WARN ) ;
	this.assertEquals( s, "WARN" , "LOG_EV_06_06 : LogEventLevel.WARN : " + s ) ;
	
	var s = vegas.logging.LogEvent.getLevelString () ;
	this.assertEquals( s, null , "LOG_EV_06_07 : undefined : " + s ) ;

	var s = vegas.logging.LogEvent.getLevelString ("") ;
	this.assertEquals( s, null , "LOG_EV_06_08 : \"\" : " + s ) ;
	
}



