Tests.vegas.logging.TestLogEventLevel = function( name ) 
{
    buRRRn.ASTUce.TestCase.call( this, name ) ;
}

Tests.vegas.logging.TestLogEventLevel.prototype = new buRRRn.ASTUce.TestCase() ;
Tests.vegas.logging.TestLogEventLevel.prototype.constructor = Tests.vegas.logging.TestLogEventLevel ;

Tests.vegas.logging.TestLogEventLevel.prototype.testSingleton = function () 
{
	this.assertTrue( vegas.logging.LogEventLevel != null  , "LOG_EV_LV_01 - singleton exist." ) ;
}

Tests.vegas.logging.TestLogEventLevel.prototype.testIsValidLevel = function () 
{
	
	var isValidLevel = vegas.logging.LogEventLevel.isValidLevel ;
	
	this.assertTrue( isValidLevel( 0 )    , "LOG_EV_LV_06_02 : isValidLevel 0 (ALL)."   ) ;
	this.assertTrue( isValidLevel( 2 )    , "LOG_EV_LV_06_02 : isValidLevel 2 (DEBUG)." ) ;
	this.assertTrue( isValidLevel( 8 )    , "LOG_EV_LV_06_03 : isValidLevel 8 (ERROR)." ) ;
	this.assertTrue( isValidLevel( 1000 ) , "LOG_EV_LV_06_04 : isValidLevel 1000 (FATAL)." ) ;
	this.assertTrue( isValidLevel( 4 )    , "LOG_EV_LV_06_05 : isValidLevel 4 (INFO)." ) ;
	this.assertTrue( isValidLevel( 6 )    , "LOG_EV_LV_06_06 : isValidLevel 6 (WARN)." ) ;
	
	this.assertFalse( isValidLevel( undefined ) , "LOG_EV_LV_06_06 : isValidLevel undefined must return false." ) ;
	this.assertFalse( isValidLevel( 12)         , "LOG_EV_LV_06_06 : isValidLevel 12 must return false." ) ;

}


