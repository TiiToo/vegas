/**
 * This default event is used in the Gateway _resolve method when the client don't use an Event object in this NetConnection.call method.
 * @author eKameleon 
 */ 
if (asgard.events.GatewayEvent == undefined) 
{
	
	/**
	 * @requires vegas.events.DynamicEvent
	 */
	require("vegas.events.DynamicEvent") ;

	/**
	 * Creates a new GatewayEvent instance.
	 */	
	asgard.events.GatewayEvent = function(type/*String*/, client/*Object*/, context ) 
	{
		vegas.events.DynamicEvent.apply(this, Array.fromArguments(arguments)) ;
		this.client = client || null ;
	}
	
	/**
	 * @extends vegas.events.DynamicEvent
	 */
	asgard.events.GatewayEvent.extend( vegas.events.DynamicEvent ) ;
 
	/**
	 * The Client reference of this event.
	 */
	asgard.events.GatewayEvent.prototype.client = null ;
	
	/**
	 * Returns a shallow copy of this object.
	 * @return a shallow copy of this object.
	 */
	asgard.events.GatewayEvent.prototype.clone = function () 
	{
		return new asgard.events.GatewayEvent( this.getType() , this.client ,this.getContext() ) ;
	}
	
}
