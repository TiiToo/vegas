/** ErrorElement
 
	AUTHOR
	
		Name : ErrorElement
		type : SSAS
		Package : vegas.errors
		Version : 1.0.0.0
		Date :  2006-05-31
		Author : ekameleon
		URL : http://www.ekameleon.net
		Mail : contact@ekameleon.net

	CONSTRUCTOR
	
		new ErrorElement(thrower, methodName:String, args:Array, fileName:String, lineNumber:Number, isNative:Boolean)
	
	METHOD SUMMARY
	
		- equals(o):Boolean
		
			Returns true if the given object is also a ErrorElement and all attributes, except the native flag, are equal.
		
		- getArguments():Array
		
			Returns an array with parameters of the thrower method.
		
		- getConstructorName():String
		
			Returns the fully qualified name of the class containing the execution point represented by this stack trace element.
		
		- getFileName():String
		
			Returns the name of the source file containing the execution point represented by this stack trace element.
		
		- getLineNumber():Number
		
			Returns the line number of the source line containing the execution point represented by this stack trace element.
		
		- getMethodName():String
		
			Returns the name of the method containing the execution point represented by this stack trace element.
		
		- isNativeMethod():Boolean
		
			Returns true if the method containing the execution point represented by this stack trace element is a native method.
		
		- toString():String
		
			Returns a string representation of this stack trace element.

	INHERIT
	
		CoreObject â†’ ErrorElement

	IMPLEMENT
	
		IEquality, IFormattable, IHashable
 
 **/
 
_global.getPackage("vegas.errors") ;

if (vegas.errors.ErrorElement == undefined) {
	
	// ----o Imports
	
	loadFile("vegas/core/CoreObject.asc") ;
	
	// ----o Constructor
 
	vegas.errors.ErrorElement = function(thrower, methodName/*String*/, args/*Array*/, fileName/*String*/, lineNumber/*Number*/, isNative/*Boolean*/) {
		this._args = args || null ;
		this._fileName = fileName || null ;
		this._lineNumber = isNaN(lineNumber) ? null : lineNumber ;
		this._isNative = isNative || null ;
		this._methodName = methodName || null ;
		this._thrower = thrower || null ;
	}
	
	// ----o Inherit
 
	var constructor = vegas.errors.ErrorElement ;
	var proto = constructor.extend( vegas.core.CoreObject ) ;

	// ----o Public Methods

	proto.equals = function (o)/*Boolean*/ {
		if (!o instanceof vegas.errors.ErrorElement) {
			return false ;
		}
		return (
			o.getConstructorName() == this.getConstructorName() 
			&& o.getConstructorPath() == this.getConstructorPath()
			&& o.getFileName() == this.getFileName()
			&& o.getLineNumber() == this.getLineNumber()
			&& o.getMethodName()== this.getMethodName()
			&& o.getThrower() == this.getThrower()
		) ;
	}
	
	proto.getArguments = function ()/*Array*/ {
		return this._args ;
	}
	
	proto.getConstructorName = function ()/*String*/ {
		return this._thrower.getConstructorName() ;
	}

	proto.getConstructorPath = function ()/*String*/ {
		return this._thrower.getConstructorPath() ;
	}
	
	proto.getFileName = function ()/*String*/ {
		return this._fileName ;
	}
	
	proto.getLineNumber = function ()/*Number*/{
		return this._lineNumber ;
	}
	
	proto.getMethodName = function ()/*String*/ {
		return this._methodName ;
	}
	
	proto.getThrower = function () {
		return this._thrower ;
	}

	proto.isNativeMethod = function ()/*Boolean*/ {
		return this._isNative ;
	}
	
	// ----o Private Properties
	
	proto._args /*Array*/ ;
	proto._fileName /*String*/ ;
	proto._lineNumber /*Number*/ ;
	proto._isNative /*Boolean*/ ;
	proto._methodName /*String*/ ;
	proto._thrower ;

	// ----o Encapsulate
 
	delete constructor ;
	delete proto ;

	// trace ("***** running vegas.errors.ErrorElement") ;

}
