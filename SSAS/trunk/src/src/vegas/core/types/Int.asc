/** Int

	AUTHOR

		Name : Int
		type : SSAS
		Package : vegas.core.types
		Version : 1.0.0.0
		Author : ekameleon
		Date : 2006-05-31
		URL : http://www.ekameleon.net
		Mail : vegas@ekameleon.net
	
	DESCRIPTION
	
		Nombre entier

	METHOD SUMMARY

		- hashCode():Number

		- toString():String
		
		- valueOf()
	
	INHERIT
	
		Number â†’ Int

	IMPLEMENTS
	
		IFormattable, IHashable
	
	EXAMPLE
	
		var i = new vegas.core.types.Int(12.25) ;
		trace("int : " + i) ;
	
**/ 

var p = _global.getPackage("vegas.core.types") ;

if (p.Int == undefined) {

	// ----o Constructor

	vegas.core.types.Int = function ( n/*Number*/) { 
		
		if (n == Infinity || n == -Infinity) {
			trace("Int constructor, 'n' value not must be Infinity or -Infinity") ;
			//throw new vegas.errors.NumberFormatError() ;
		} else {
			this._int = n - n%1 ;
		}
	}

	var constructor = vegas.core.types.Int ;
	var proto = constructor.extend(Number) ;
	
	// ----o Public Methods
	
	proto.toString = function() /*String*/ {
		return this._int.toString() ;
	}

	proto.valueOf = function() {
		return this._int ;
	}

	// ----o Private Properties
	
	proto._int /*Number*/ = null ;

	// ----o Encapsulate

	delete constructor ;
	delete proto ;
	
	//trace ("***** running vegas.core.types.Int") ;
	
}

delete p ;