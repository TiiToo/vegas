	vegas.logging.SOSType.INFO_COLOR /*Number*/ = 0xD2FAB8 ;
	
	vegas.logging.SOSType.PORT /*Number*/ = 4444 ;
	
	vegas.logging.SOSType.WARN_COLOR /*Number*/ = 0xFDFDB5 ;
	
