/** LinearQueue

	AUTHOR

		Name : LinearQueue
		type : SSAS
		Package : vegas.data.queue
		Version : 1.0.0.0
		Author : ekameleon
		Date : 2006-01-09
		URL : http://www.ekameleon.net
		Mail : vegas@ekameleon.net
		
	CONSTRUCTOR
	
		new vegas.data.queue.LinearQueue( [ar] ) ;

	METHODS
	
		- dequeue() : Retrieves and removes the head of this queue.
		
		- element() : Retrieves, but does not remove, the head of this queue.
		
		- enqueue(o) : Inserts the specified element into this queue, if possible.
		
		- peek() : Retrieves, but does not remove, the head of this queue, returning null if this queue is empty.
		
		- poll() : Retrieves and removes the head of this queue.
		
		- toArray():Array
		
		- toSource():String
		
		- toString():String
		
	INHERIT
	
		CoreObject > AbstractCollection > LinearQueue

	IMPLEMENTS
	
		Cloneable, Collection, Iterable, Queue, Serializable, ToString
	
	EXAMPLE
	
		var q = new vegas.data.queue.LinearQueue(["item0", "item1"])  ;
		trace ("queue size : " + q.size()) ;
		trace ("enqueue item2 : " + q.enqueue ("item2")) ;
		trace ("enqueue item3 : " + q.enqueue ("item3")) ;
		trace ("enqueue item4 : " + q.enqueue ("item4")) ;
		trace ("------- element") ;
		trace ("element : " + q.element()) ;
		trace ("peek : " + q.peek()) ;
		trace ("------- dequeue") ;
		trace ("dequeue : " + q.dequeue()) ;
		trace ("------- iterator") ;
		var it = q.iterator() ;
		while (it.hasNext()) {
			trace (it.next()) ;
		}
		trace ("------- queue Format") ;
		trace (q.size() + " >> " + q) ;
		trace ("queue.toSource : " + q.toSource()) ;
	
*/ 

if (vegas.data.queue.LinearQueue == undefined) {
	
	// ----o Constructor

	vegas.data.queue.LinearQueue = function ( ar/*Array*/ ) { 
		vegas.data.collections.AbstractCollection.apply(this, [ar]) ;
	}

	// ----o Inherit

	vegas.data.queue.LinearQueue.extend(vegas.data.collections.AbstractCollection) ;

	// ----o Public Methods
	
	vegas.data.queue.LinearQueue.prototype.clone = function () {
		return new vegas.data.queue.LinearQueue(this._a) ;
	}
	
	vegas.data.queue.LinearQueue.prototype.dequeue = function () /*Boolean*/ {
		return this.poll() != null  ;
	}
	
	vegas.data.queue.LinearQueue.prototype.element = function () {
		return this._a[0] ;
	}

	vegas.data.queue.LinearQueue.prototype.enqueue = function (o) /*Boolean*/ {
		if (o == undefined) return false ;
		this._a.push(o) ;
		return true ;
	}

	vegas.data.queue.LinearQueue.prototype.peek = function () {
		if (this.isEmpty()) return null ;
		return this._a[0] ;
	}

	vegas.data.queue.LinearQueue.prototype.poll = function () {
		if (this.isEmpty()) return null ;
		return this._a.shift() ;
	}

	vegas.data.queue.LinearQueue.prototype.toString = function () /*String*/ {
		return (new vegas.data.queue.QueueFormat()).formatToString(this) ;
	}
	
	// ----o Encapsulate

	//trace ("***** running vegas.data.queue.LinearQueue") ;
	
}