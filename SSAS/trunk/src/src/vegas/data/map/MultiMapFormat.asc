/** MultiMapFormat

	AUTHOR

		Name : MultiMapFormat
		type : SSAS
		Package : vegas.data.map
		Version : 1.0.0.0
		Author : ekameleon
		Date : 2006-01-18
		URL : http://www.ekameleon.net
		Mail : vegas@ekameleon.net
		
	METHOD SUMMARY

		- formatToString( o ):String
		
		- toString():String

	INHERIT
	
		Object > MultiMapFormat

	IMPLEMENTS
	
		Format
	
**/ 

_global.getPackage("vegas.data.map") ;

if (vegas.data.map.MultiMapFormat == undefined) {
	
	// ----o Imports
	
	loadFile("vegas/core/CoreObject.asc") ;
	
	// ----o Constructor

	vegas.data.map.MultiMapFormat = function () { 
		//
	}

	var constructor = vegas.data.map.MultiMapFormat ;
	var proto = constructor.extend(vegas.core.CoreObject) ;

	// ----o Public Methods
		
	proto.formatToString = function ( map /*MultiMap*/ ) /*String*/ {
		var r /*String*/ = "{" ;
		// TODO ici tester que l'objet h√©rite de MultiMap
		if (map.valueIterator() != undefined && map.keyIterator() != undefined) {
			var kIterator = map.keyIterator() ;
			var vIterator = map.valueIterator() ;
			while  (kIterator.hasNext()) {
				r += kIterator.next().toString() + ":" + vIterator.next().toString() ;
				if (kIterator.hasNext()) r += "," ;
			}
		}
		r += "}";
		return r ;
	}

	// ----o Encapsulate

	delete constructor ;
	delete proto ;
	
	//trace ("***** running vegas.data.map.MultiMapFormat") ;
	
}