_global.getPackage = function ( path /*String*/ ) {
	var a = path.split('.');
    var scope = _global ;
	var l = a.length ;
	for (var i = 0 ; i < l ; i++) {
      var name = a[i];
      if ( ! scope.hasOwnProperty(name) ) scope[name] = {} ;
      scope = scope[name] ;
   }
   return scope ;
}