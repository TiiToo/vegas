/* ----- Class AbstractSet

	AUTHOR

		Name : AbstractSet
		type : SSAS
		Package : vegas.data.set
		Version : 1.0.0.0
		Author : ekameleon
		Date : 2006-01-28
		URL : http://www.ekameleon.net
		Mail : contact@ekameleon.net
		
	METHOD SUMMARY

		- clear()
		
		- contains(o)
		
		- containsAll(c:Collection)
		
		- equals(o):Boolean
		
		- get(id)
		
		- indexOf(o)
		
		- insert(o)
		
		- insertAll(c:Collection)
		
		- isEmpty()
		
		- iterator()
		
		- remove()
		
		+ removeAll(c:Collection)
		
		- retainAll(c:Collection)
		
		- size():Number
		
		- toArray():Array
		
		- toSource():String
		
		- toString():String

	INHERIT
	
		Object > AbstractCollection > SimpleCollection > AbstractSet

	IMPLEMENTS
	
		Collection, Set, Serializable, ToString
	
	SEE ALSO 
	
		Core2 Library
	
---------- */ 

var namespace = _global.getPackage("vegas.data.set") ;

if (namespace.AbstractSet == undefined) {

	loadFile("vegas/data/collections/SimpleCollection.asc") ;

	// ----o Constructor

	vegas.data.set.AbstractSet = function () { 
		//
	}

	var constructor = vegas.data.set.AbstractSet ;
	var proto = constructor.extend(vegas.data.collections.SimpleCollection) ;

	// ----o Public Methods
		
	proto.equals = function (o) /*Boolean*/ {
		if (o == this) return true ;
		if ( ! (o instanceof this.constructor) ) {
			return false ;
		}
		if (o.size() == undefined || o.size() != this.size()) return false ;
		return this.containsAll(o) ;
	}
		
	proto.removeAll = function (c /*Collection*/ ) /*Boolean*/ {
		var b /*Boolean*/ = false ;
		if (this.size() > c.size()) {
			var it /*Iterator*/ = c.iterator() ;
			while (it.hasNext()) {
				b = this.remove(it.next()) ;
			}
		} else {
			var it /*Iterator*/ = this.iterator() ;
			while (it.hasNext()) {
				if (c.contains(it.next())) {
					it.remove() ;
					b = true ;
				}
			}
			
		}
		return b ;
	}

	// ----o Encapsulate

	delete constructor ;
	delete proto ;
	
	//trace ("***** running vegas.data.set.HashSet") ;
	
}

delete namespace ;