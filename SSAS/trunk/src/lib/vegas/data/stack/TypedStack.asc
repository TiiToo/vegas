/* ----- Class TypedStack

	AUTHOR

		Name : TypedStack
		type : SSAS
		Package : vegas.data.stack
		Version : 1.0.0.0
		Author : ekameleon
		Date : 2006-01-16
		URL : http://www.ekameleon.net
		Mail : vegas@ekameleon.net
		
	CONSTRUCTOR
	
		var ta:TypedStack = new TypedStack( type:Function , stack:Stack) 

	METHOD SUMMARY
	
		- clear()
		
		- clone()
		
		- getType()
		
		- isEmpty()
		
		- iterator()
		
		- peek()
		
		- pop()
		
		- push(o)
		
		- search(o):Number
		
		- setType(type:Function)
		
		- size():Number
		
		- toArray():Array ;
		
		- toString():String

	INHERIT
	
		Object > AbstractTypeable> TypedStack

	IMPLEMENTS
	
		Cloneable, Iterable, Serializable, Typeable, Validator	
	
	EXAMPLE	

		load("lib/vegas.asc") ;

		var s = new vegas.data.stack.SimpleStack() ;
		s.push("item1") ;
		s.push("item2") ;
		s.push("item3") ;
		
		var ts = new vegas.data.stack.TypedStack(String, s) ;
		ts.push("item4") ;
		trace ("stack >> " + ts.size() + " : " + ts) ;
		trace("stack.toSource : " + ts.toSource()) ;
		ts.push(2) ;

---------- */ 

var namespace = _global.getPackage("vegas.data.stack") ;

if (namespace.TypedStack == undefined) {

	loadFile("vegas/util/AbstractTypeable.asc") ;
	loadFile("vegas/util/TypeUtil.asc") ;

	// ----o Constructor

	vegas.data.stack.TypedStack = function ( type /*Function*/ , stack /*Stack*/ ) { 
		this.__constructor__.call(this, type) ;
		if (stack == null) throw new Error("IllegalArgumentError:Argument 'stack' must not be 'null' or 'undefined'") ;
		if (stack.size() > 0) {
			var it = stack.iterator() ;
			while ( it.hasNext() ) this.validate(it.next()) ;
		}
		this._stack = stack ;
	}

	var constructor = vegas.data.stack.TypedStack ;
	var proto = constructor.extend(vegas.util.AbstractTypeable) ;

	// ----o Public Methods
	
	proto.clear = function () {
		this._stack.clear() ;
	}
	
	proto.clone = function () {
		return new vegas.data.stack.TypedStack(this.getType(), this._queue.clone()) ;
	}

	proto.isEmpty = function () /*Boolean*/ {
		return this._stack.isEmpty() ;
	}

	proto.iterator = function ()/*Iterator*/ {
		return this._stack.iterator() ;
	}

	proto.peek = function () {
		return this._stack.peek() ;
	}
	
	proto.pop = function () {
		return this._stack.poll() ;
	}
	
	proto.push = function (o) {
		this.validate(o) ;
		this._stack.push(o) ;
	}
	
	proto.search = function (o) /*Number*/ {
		return this._search(o) ;
	}

	proto.setType = function (type /*Function*/) /*Void*/ {
		this.__constructor__.prototype.setType.call(this, type) ;
		this._stack.clear() ;
	}

	proto.size = function ()/*Number*/ {
		return this._stack.size() ;
	}
	
	proto.toArray = function ()/*Array*/ {
		return this._stack.toArray() ;
	}

	proto.toSource = function (indent/*Number*/, indentor/*String*/)/*String*/ {
		var sourceA/*String*/ = vegas.util.TypeUtil.toString(this._type) ;
		var sourceB/*String*/ = this._stack.toSource() ;
		return "new vegas.data.stack.TypedStack(" + sourceA + "," + sourceB + ")" ;
	}

	proto.toString = function ()/*String*/ {
		return this._stack.toString() ;
	}
	
	// ----o Encapsulate

	delete constructor ;
	delete proto ;
	
	//trace ("***** running vegas.data.stack.TypedStack") ;
	
}

delete namespace ;