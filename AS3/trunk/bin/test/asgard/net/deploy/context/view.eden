{
    objects : 
    [
        {
            id         : "my_field" ,
            type       : "TextField"  , // this class is created and embed in the library of the swf.
            properties : 
            [
                { name : "autoSize"          , value  : "left" } ,
                { name : "defaultTextFormat" , value  : new flash.text.TextFormat('arial', 12, 0xFFFFFF) } ,
                { name : "filters"           , config : "filters" } ,
                { name : "x"                 , value  : 10 } ,
                { name : "y"                 , value  : 10 } ,
                { name : "text"              , value  : "hello world" } 
            ]
        }
        ,
        {
            id         : "my_sprite" ,
            type       : "{display}.CoreSprite" ,
            singleton  : true  ,  // must be a singleton to test the ObjectConfig.identify flag. 
			identify   : true
        }
        ,
        {
            id         : "square" ,
            type       : "Square" ,
            singleton  : true     ,
            properties : 
            [
                { name : "filters" , config : "filters" } ,  
                { name : "x"       , value  : 50        } ,
                { name : "y"       , value  : 50        }
            ]
            ,
            methods :
            [
                { name:"addChild" , arguments:[ { ref:"my_field" } ] }
            ]
        }        
    ]
}