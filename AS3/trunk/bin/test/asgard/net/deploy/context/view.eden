objects =
[
    {
        id                  : "formatter" ,
		type                : "String"    ,
		staticFactoryMethod :
		{
			type      : "system.Strings" , 
			name      : "format" ,
			arguments : [ { config:"pattern" } , { config:"pseudo" } ]
		}
	}
	,
    {
        id         : "my_field" ,
        type       : "TextField"  ,
        properties : 
        [
            { name : "autoSize"          , value  : "left" } ,
            { name : "defaultTextFormat" , value  : new flash.text.TextFormat('arial', 12, 0xFFFFFF) } ,
            { name : "filters"           , config : "filters" } ,
            { name : "x"                 , value  : 10 } ,
            { name : "y"                 , value  : 10 } ,
            { name : "text"              , ref    : "formatter" } 
        ]
    }
    ,
    {
        id         : "my_square" ,
        type       : "Square" ,
        properties : 
        [
            { name : "filters" , config : "filters" } 
        ]
    } 
    ,
    {
        id         : "my_container" ,
        type       : "{display}.CoreSprite" ,
        singleton  : true  , 
		lazyInit   : true  ,
        identify   : true  ,
        properties : 
        [
            { name : "x" , value  : 50 } ,
            { name : "y" , value  : 50 }
        ]
		,
        methods :
        [
			{ name:"addChild" , arguments:[ { ref:"my_square" } ] } ,
            { name:"addChild" , arguments:[ { ref:"my_field"  } ] }
        ]		
    }	
		
] ;
