<?xml version="1.0" encoding="utf-8"?>

<flex-config>
    <compiler>
        <source-path>
            <path-element>../../src/</path-element>
        </source-path>
        
        <namespaces>
            <namespace>
                <uri>http://code.google.com/p/core/2010</uri>
                <manifest>./manifests/core_manifest.xml</manifest>
            </namespace>
            <namespace>
                <uri>http://code.google.com/p/system/2010</uri>
                <manifest>./manifests/system_manifest.xml</manifest>
            </namespace>
            <namespace>
                <uri>http://code.google.com/p/graphics/2010</uri>
                <manifest>./manifests/graphics_manifest.xml</manifest>
            </namespace>
            <namespace>
                <uri>http://code.google.com/p/vegas/2010</uri>
                <manifest>./manifests/vegas_manifest.xml</manifest>
            </namespace>
            <namespace>
                <uri>http://code.google.com/p/lun-as/2010</uri>
                <manifest>./manifests/lunas_manifest.xml</manifest>
            </namespace>
            <namespace>
                <uri>http://code.google.com/p/calista/2010</uri>
                <manifest>./manifests/calista_manifest.xml</manifest>
            </namespace>
        </namespaces>
        
        <strict>true</strict>
        <optimize>true</optimize>
    </compiler>
    
    <include-namespaces>
        <uri>http://code.google.com/p/core/2010</uri>
        <uri>http://code.google.com/p/system/2010</uri>
        <uri>http://code.google.com/p/graphics/2010</uri>
        <uri>http://code.google.com/p/vegas/2010</uri>
        <uri>http://code.google.com/p/lun-as/2010</uri>
        <uri>http://code.google.com/p/calista/2010</uri>
    </include-namespaces>
    
    <target-player>10.0.0</target-player>
    
    <directory>false</directory>
    
    <output>../../libs/vegas.swc</output>
    
    <metadata>
        <title>The AS3 VEGAS framework - version 1.7.0.1503</title>
        <description>All class of VEGAS and this extensions</description>
        <publisher>http://www.ekameleon.net</publisher>
        <creator>Marc ALCARAZ (aka eKameleon)</creator>
        <language>EN</language>
    </metadata>

</flex-config>
